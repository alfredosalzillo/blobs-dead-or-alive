/* lib/random.ts */
const mod6 = (async () => {
    const random = (min, max, float = false) => {
        const val = Math.random() * (max - min) + min;
        if (float) {
            return val;
        }
        return Math.floor(val);
    };
    function randomItem(list) {
        return list[random(0, list.length)];
    }
    return { random, randomItem };
})();
